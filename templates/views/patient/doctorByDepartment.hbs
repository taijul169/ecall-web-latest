{{patientHeader userData.[0].PatientID userData.[0].Photo userData.[0].FirstName }}

	{{#if message}}
        <div class="alert {{message.type}}" role="alert">
            {{message.message}}
        </div>
    {{/if}}
<div class="breadcrumb-bar">
    <div class="container-fluid">
        <div class="row align-items-center">
            <div class="col-md-12 col-12">
                <nav aria-label="breadcrumb" class="page-breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Dashboard</li>
                    </ol>
                </nav>
                <h2 class="breadcrumb-title">Dashboard</h2>
            </div>
        </div>
    </div>
</div>


<div class="content">
    <div class="container-fluid">
        <div class="row">

            <div class="col-md-5 col-lg-4 col-xl-3 theiaStickySidebar">
                <div class="profile-sidebar">
                    <div class="widget-profile pro-widget-content">
                            <div class="profile-info-widget">
                                    <a href="#" class="booking-doc-img">
                                        <img src="{{userData.[0].Photo}}" alt="User Image">
                                    </a>
                                    <div class="profile-det-info">
                                        <h3>{{userData.[0].FirstName}}</h3>
                                        <div class="patient-details">
                                            <h5><i class="fas fa-birthday-cake"></i> 24 Jul 1983, 38 years</h5>
                                            <h5 class="mb-0"><i class="fas fa-map-marker-alt"></i> {{userData.[0].PrimaryAddress}}</h5>
                                        </div>
                                    </div>
                                </div>
                    </div>
                    <div class="dashboard-widget">
                        {{patientSideNav userData.[0].PatientID}}
                    </div>
                </div>
            </div>

            <div class="col-md-7 col-lg-8 col-xl-9">
                <div class="row mx-0">
                       {{#each dataDoclist}}
                            <div class="card">
                                <div class="card-body">
                                    <div class="doctor-widget">
                                        <div class="doc-info-left">
                                            <div class="doctor-img">
                                                <a href="doctor-profile.html">
                                                    <img src="{{this.Photo}}" class="img-fluid"
                                                        alt="User Image">
                                                </a>
                                            </div>
                                            <div class="doc-info-cont">
                                                <h4 class="doc-name"><a href="doctor-profile.html">Dr. {{this.FirstName}} {{this.LastName}}</a></h4>
                                                <p class="doc-speciality">MDS - Periodontology and Oral Implantology, BDS
                                                </p>
                                                {{departmentNameDisplay ../ departmentData this.Department}}
                                                <div class="rating">
                                                    {{ reviewShow this.averageRating}}
                                                    <span class="d-inline-block average-rating">({{this.NumberOfRating}})</span>
                                                </div>
                                                <div class="clinic-details">
                                                    <p class="doc-location"><i class="fas fa-map-marker-alt"></i> {{this.PrimaryAddress}}</p>
                                                    <ul class="clinic-gallery">
                                                        <li>
                                                            <a href="/assets/img/features/feature-01.jpg"
                                                                data-fancybox="gallery">
                                                                <img src="/assets/img/features/feature-01.jpg" alt="Feature">
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a href="/assets/img/features/feature-02.jpg"
                                                                data-fancybox="gallery">
                                                                <img src="/assets/img/features/feature-02.jpg" alt="Feature">
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a href="/assets/img/features/feature-03.jpg"
                                                                data-fancybox="gallery">
                                                                <img src="/assets/img/features/feature-03.jpg" alt="Feature">
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a href="/assets/img/features/feature-04.jpg"
                                                                data-fancybox="gallery">
                                                                <img src="/assets/img/features/feature-04.jpg" alt="Feature">
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </div>
                                                <div class="clinic-services d-none">
                                                    <span>Dental Fillings</span>
                                                    <span> Whitneing</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="doc-info-right">
                                            <div class="clini-infos">
                                                <div class="fav-unfav-box-custom w-100 ">
                                                    {{makeFavUnFav ../favdoclist this.DocNurID ../userData.[0].PatientID}}
                                                    
                                               </div>
                                                <ul>
                                                    <li><i class="far fa-thumbs-up"></i> 98%</li>
                                                    <li><i class="far fa-comment"></i> {{this.NumberOfRating}} Feedback</li>
                                                    <li><i class="fas fa-map-marker-alt"></i> {{this.PrimaryAddress}}</li>
                                                    <li><i class="far fa-money-bill-alt"></i> Home - <strong>৳ {{this.Home}}</strong> <i
                                                            class="fas fa-info-circle" data-bs-toggle="tooltip"
                                                            title="Lorem Ipsum"></i> </li>
                                                    <li><i class="far fa-money-bill-alt"></i> Online - <strong>৳ {{this.Online}}</strong>  <i
                                                            class="fas fa-info-circle" data-bs-toggle="tooltip"
                                                            title="Lorem Ipsum"></i> </li>
                                                </ul>
                                            </div>
                                            <div class="clinic-booking">
                                                <a class="view-pro-btn" href="/singledoctorprofileview/{{this.DocNurID}}">View Profile</a>
                                                <a class="apt-btn" href="/checkout/{{this.DocNurID}}">Book Appointment</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        {{/each}}
						<div class="load-more text-center">
							<a class="btn btn-primary btn-sm" href="javascript:void(0);">Load More</a>
						</div>
                </div>
            </div>
        </div>
    </div>
</div>


