                                        <h5 class="card-title">{{this.FirstName}} {{this.LastName}}</h5>
                                       <table class="table table-striped">
                                            <thead>
                                                <tr>
                                                <th scope="col">ID</th>
                                                <th scope="col">Date</th>
                                                <th scope="col">Time</th>
                                                <th scope="col">Payment</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                <th scope="row">#{{this.appointmentID}}</th>
                                                <td>{{this.BookingData}}</td>
                                                <td>{{this.BookingTime}}</td>
                                                <td>{{this.PaymentMethod}}</td>
                                                </tr>
                                             
                                              
                                            </tbody>
                                         </table>
                                    </div>
                                    <div class="card-footer d-flex justify-content-evenly  ">
                                        <a href="/chat-doctor/{{this.DocNurID}}/{{this.PatientID}}" onclick="appAccept('{{this.PatientID}}')" class="btn-sm btn-outline-primary"><i class="fas fa-phone"></i></a>
                                        <a href="/chat-doctor-video/{{this.DocNurID}}/{{this.PatientID}}" onclick="appAccept('{{this.PatientID}}')" class="btn-sm btn-outline-secondary"><i class="fas fa-video"></i></a>
                                        <a href="/chat-doctor/{{this.DocNurID}}/{{this.PatientID}}" onclick="appAccept('{{this.PatientID}}')" class="btn-sm btn-outline-success"><i class="fas fa-ellipsis-v"></i></a>
                                     
                                    </div>
                                   
                                    <span class="badge bg-success status">Done</span>
                                </div>
                                </a>
                                  
                            </div>

